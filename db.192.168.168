;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	localhost. root.localhost. (
			2020062201	; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

; name servers - NS records
      IN      NS      frontnode.rendebeest.lan.
	  
; PTR Records
1   IN      PTR     frontnode.rendebeest.lan.    ; 192.168.168.1

;

