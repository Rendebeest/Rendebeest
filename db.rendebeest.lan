;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	frontnode.rendebeest.lan. admin.rendebeest.lan. (
			2020062201	; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

; name servers - NS records
    IN      NS      frontnode.rendebeest.lan.

; A records
frontnode.rendebeest.lan.          IN      A       192.168.168.1
